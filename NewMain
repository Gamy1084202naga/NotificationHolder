local GUI = game:GetService("CoreGui"):FindFirstChild("ROMHUBNofitication") 
  
 if not GUI then 
  
     local RNofitication = Instance.new("ScreenGui") 
  
     local RNofiticationUIListLayout = Instance.new("UIListLayout") 
  
     RNofitication.Name = "RNofitication" 
  
     RNofitication.Parent = game.CoreGui 
  
     RNofitication.ZIndexBehavior = Enum.ZIndexBehavior.Sibling 
  
     RNofitication.ResetOnSpawn = false 
  
      
  
     RNofiticationUIListLayout.Name = "RUIListLayout" 
  
     RNofiticationUIListLayout.Parent = RNofitication 
  
     RNofiticationUIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Right 
  
     RNofiticationUIListLayout.SortOrder = Enum.SortOrder.LayoutOrder 
  
     RNofiticationUIListLayout.VerticalAlignment = Enum.VerticalAlignment.Bottom 
  
RNofiticationUIListLayout.Padding = UDim.new(0, 5)
 

 else 
  
 end
